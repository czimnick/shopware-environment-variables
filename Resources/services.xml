<?xml version="1.0" ?>

<container
        xmlns="http://symfony.com/schema/dic/services"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <service id="shopware_environment_variables.config_reader"
                 class="ShopwareEnvironmentVariables\Reader"
                 decorates="shopware.plugin.config_reader"
                 public="false">
            <argument type="service" id="shopware_environment_variables.config_reader.inner"/>
            <argument>%shopware.custom%</argument>
        </service>
    </services>
</container>
